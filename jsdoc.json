{
    "source": {
      "include": ["./api", "./pages", "./layouts", "./docs/components", "./pages/report", "./pages/customer"],
      "includePattern": "\\.(vue|js)$",
      "exclude": ["node_modules"]
    },
    "plugins": [
      "plugins/markdown",
      "node_modules/jsdoc-vue-component",
      "node_modules/jsdoc-vuejs"
    ],
    "opts": {
      "destination": "docs"
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
        "outputSourceFiles": true
      },
      "path": "ink-docstrap",
      "theme": "cerulean",
      "navType": "vertical",
      "linenums": true,
      "collapseSymbols": false,
      "inverseNav": true
    }
  }